<AnchorPane prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seatSelection">
   <children>
      <AnchorPane fx:id="pane" layoutX="150.0" layoutY="50.0" prefHeight="750.0" prefWidth="1050.0">
         <children>
            <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#e2e6e9" height="122.0" layoutX="75.0" layoutY="580.0" stroke="BLACK" strokeType="INSIDE" width="900.0" />
            <Label layoutX="479.0" layoutY="623.0" prefHeight="30.0" prefWidth="92.0" text="SCREEN">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
            <Button layoutX="839.0" layoutY="525.0" mnemonicParsing="false" onAction="#confirmSelection" text="Confirm Seat Selection" />
         </children>
      </AnchorPane>
      <Button fx:id="logOut" layoutX="17.0" layoutY="764.0" mnemonicParsing="false" onAction="#logOut" text="Log Out" />
      <Button fx:id="changePassword" layoutX="17.0" layoutY="732.0" mnemonicParsing="false" onAction="#changePassword" text="Change Password" />
      <Button layoutX="17.0" layoutY="700.0" mnemonicParsing="false" onAction="#goToSearch" text="Go Back To Search" />
      <Label fx:id="name" alignment="CENTER_RIGHT" layoutX="720.0" layoutY="7.0" prefHeight="35.0" prefWidth="466.0" text="Welcome user">
